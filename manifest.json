{
  "name": "JS4Hacking",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "assets/logo.png",
    "default_title": "JS4Hacking"
  },
  "icons": {
    "16": "assets/logo.png",
    "48": "assets/logo.png",
    "128": "assets/logo.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
           "injector.js",
           "Modules/logger.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "main.js",
        "Modules/cookie_reflections_logger.js",
        "Modules/storage_reflections_logger.js",
        "Modules/url_params_reflections_logger.js",
        "Modules/golden_bug_detector.js",
        "Modules/logger.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
