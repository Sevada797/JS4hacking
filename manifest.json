{
  "name": "JS4Hacking",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [],
  "action": {
    "default_icon": "assets/logo.png",
    "default_title": "JS4Hacking"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["injector.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["main.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
